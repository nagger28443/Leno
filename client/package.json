{
  "name": "leno",
  "version": "1.0.0",
  "description": "Simple blog",
  "main": "app.jsx",
  "author": "nagger",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:dll": "rimraf dll",
    "dll:dev": "cross-env NODE_ENV=development npm run clean:dll && webpack --config build/webpack.dll.js --progress",
    "dll:prod": "cross-env NODE_ENV=production npm run clean:dll && webpack --config build/webpack.dll.js -p --progress",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production npm run clean:dist && webpack --config build/webpack.config.js -p --progress",
    "test": "jest --watch"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js?(x)"
    ]
  },
  "repository": "git@github.com:nagger28443/Leno.git",
  "private": false,
  "dependencies": {
    "axios": "^0.18.0",
    "dateformat": "^3.0.3",
    "lodash": "^4.17.10",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "query-string": "^6.0.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.3.5",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.7",
    "babel-plugin-mobx-deep-action": "^1.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "ip": "^1.1.5",
    "jest": "^23.4.1",
    "style-loader": "^0.20.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  }
