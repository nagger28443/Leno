{
  "name": "leno",
  "version": "1.0.0",
  "description": "Simple blog",
  "main": "app.js",
  "author": "nagger",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:dll": "rimraf dll",
    "dll:dev": "cross-env NODE_ENV=development npm run clean:dll && webpack --config build/webpack.dll.js --progress",
    "dll:prod": "cross-env NODE_ENV=production npm run clean:dll && webpack --config build/webpack.dll.js -p --progress",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production npm run clean:dist && webpack --config build/webpack.config.js -p --progress"
  },
  "repository": "git@github.com:nagger28443/Leno.git",
  "license": "none",
  "private": false,
  "dependencies": {
    "cross-env": "^5.1.6",
    "eslint-plugin-prettier": "^2.6.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "lodash": "^4.17.10",
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "prettier": "^1.13.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-jss": "^8.4.0",
    "react-router-dom": "^4.2.2",
    "stylus": "^0.54.5",
    "webpack": "^4.10.2",
    "webpack-dev-server": "^3.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "8",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.21.0",
    "stylus-loader": "^3.0.2",
    "webpack-cli": "^2.1.4"
  }
}
